{
    "executionRoleArn": "ecsTaskExecutionRole",
    "containerDefinitions": [
        {
        "name": "web",
        "image": "<IMAGE1_NAME>",
        "portMappings": [
            {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
            }
        ],
        "essential": true
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "web"
}
